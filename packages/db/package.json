{
  "name": "@grantoz/db",
  "type": "module",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
      "src/**"
  ],  
  "scripts": {
    "build": "swc --strip-leading-paths ./src -d ./dist",
    "clean": "rm -rf dist",
