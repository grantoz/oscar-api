    "db:push": "prisma db push --skip-generate",
    "db:migrate:dev": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1"
  },
  "devDependencies": {
    "@grantoz/ts-config": "*",
    "typescript": "^5.5.4",
    "prisma": "^5.19.1"
  }
}
