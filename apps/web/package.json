    "@types/node": "^22.5.2",
    "eslint": "^9.10.0",
    "glob": "^11.0.0",
    "prisma": "^5.19.1",
    "tsx": "^4.19.1",
    "turbo": "^2.1.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@grantoz/db": "*",
